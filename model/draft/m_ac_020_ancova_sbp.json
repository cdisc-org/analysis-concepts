{
  "AC_ID": "M_AC_020",
  "AC_NAME": "ANCOVA Change from Baseline SBP Week 8",
  "AC_PURPOSE": "Estimate treatment effect on systolic blood pressure at Week 8 adjusted for baseline",
  
  "ONTOLOGY": {
    "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000128",
    "STATO_LABEL": "analysis of covariance",
    "ADDITIONAL_IRIS": [
      {
        "IRI": "http://purl.obolibrary.org/obo/STATO_0000256",
        "ONTOLOGY": "STATO",
        "LABEL": "linear model"
      }
    ]
  },
  
  "INPUTS": [
    {
      "INPUT_ID": "IN_001",
      "SOURCE_AC": "D_AC_011",
      "SOURCE_VARIABLE": "CHG",
      "ROLE": "dependent_variable",
      "REQUIRED": true,
      "DATA_TYPE": "Numeric",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000175"
    },
    {
      "INPUT_ID": "IN_002",
      "SOURCE_AC": "D_AC_010",
      "SOURCE_VARIABLE": "BASE",
      "ROLE": "covariate",
      "REQUIRED": true,
      "DATA_TYPE": "Numeric",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000474"
    },
    {
      "INPUT_ID": "IN_003",
      "SOURCE_AC": null,
      "SOURCE_VARIABLE": "TRT01P",
      "ROLE": "fixed_effect_treatment",
      "REQUIRED": true,
      "DATA_TYPE": "Character",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000251"
    },
    {
      "INPUT_ID": "IN_004",
      "SOURCE_AC": null,
      "SOURCE_VARIABLE": "REGION",
      "ROLE": "fixed_effect_stratification",
      "REQUIRED": false,
      "DATA_TYPE": "Character",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000251"
    }
  ],
  
  "OUTPUTS": [
    {
      "OUTPUT_ID": "OUT_001",
      "VARIABLE_NAME": "LSMEAN",
      "DESCRIPTION": "Adjusted mean for each treatment",
      "DATA_TYPE": "Numeric",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000343"
    },
    {
      "OUTPUT_ID": "OUT_002",
      "VARIABLE_NAME": "LSMEAN_DIFF",
      "DESCRIPTION": "Adjusted mean difference (Active - Placebo)",
      "DATA_TYPE": "Numeric",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000175"
    },
    {
      "OUTPUT_ID": "OUT_003",
      "VARIABLE_NAME": "CI_LOWER",
      "DESCRIPTION": "95% CI lower bound",
      "DATA_TYPE": "Numeric",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000315"
    },
    {
      "OUTPUT_ID": "OUT_004",
      "VARIABLE_NAME": "CI_UPPER",
      "DESCRIPTION": "95% CI upper bound",
      "DATA_TYPE": "Numeric",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000314"
    },
    {
      "OUTPUT_ID": "OUT_005",
      "VARIABLE_NAME": "PVAL",
      "DESCRIPTION": "P-value for treatment effect",
      "DATA_TYPE": "Numeric",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000051"
    },
    {
      "OUTPUT_ID": "OUT_006",
      "VARIABLE_NAME": "R_SQUARED",
      "DESCRIPTION": "R-squared for model fit",
      "DATA_TYPE": "Numeric",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000282"
    }
  ],
  
  "METHOD": {
    "IMPLEMENTATION": "stats::lm",
    "PARAMETERS": {
      "formula": "CHG ~ TRT01P + BASE + REGION",
      "estimation_method": "ordinary_least_squares",
      "confidence_level": 0.95,
      "contrasts": [
        {
          "name": "Active vs Placebo",
          "specification": "TRT01P='Active' - TRT01P='Placebo'"
        }
      ]
    }
  },
  
