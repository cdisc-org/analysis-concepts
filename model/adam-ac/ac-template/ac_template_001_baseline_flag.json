{
  "AC_ID": "T_AC_001",
  "AC_NAME": "Baseline Record Flag",
  "AC_PURPOSE": "Identify and flag baseline observation for each subject and parameter",
  "ONTOLOGY": {
    "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000474",
    "STATO_LABEL": "baseline value identification datum"
  },
  "INPUTS": [
    {
      "INPUT_ID": "IN_001",
      "SOURCE_AC_TEMPLATE": null,
      "SOURCE_CLASS_VARIABLE": "AVAL",
      "ROLE": "measurement_value",
      "REQUIRED": true,
      "DATA_TYPE": "Numeric",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000251"
    },
    {
      "INPUT_ID": "IN_002",
      "SOURCE_AC_TEMPLATE": null,
      "SOURCE_CLASS_VARIABLE": "ADT",
      "ROLE": "measurement_date",
      "REQUIRED": true,
      "DATA_TYPE": "Date",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000093"
    },
    {
      "INPUT_ID": "IN_003",
      "SOURCE_AC_TEMPLATE": null,
      "SOURCE_CLASS_VARIABLE": "TRTSDT",
      "ROLE": "reference_date",
      "REQUIRED": true,
      "DATA_TYPE": "Date",
      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000093"
    },
    {
      "INPUT_ID": "IN_004",
      "SOURCE_AC_TEMPLATE": null,
