      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000001"
    }
  ],
  "OUTPUTS": [
    {
      "OUTPUT_ID": "OUT_001",
      "CLASS_VARIABLE_NAME": "ABLFL",
