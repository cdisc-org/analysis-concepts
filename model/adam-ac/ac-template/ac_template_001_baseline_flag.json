      "STATO_IRI": "http://purl.obolibrary.org/obo/STATO_0000474"
    }
  ],
  "METHOD": {
    "IMPLEMENTATION": "custom_function.flag_baseline",
    "PARAMETERS": {
      "selection_rule": "last",
      "temporal_constraint": "ADT <= TRTSDT",
      "grouping": "USUBJID",
      "flag_value": "Y",
      "missing_handling": "exclude"
